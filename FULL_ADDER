module full_adder_gate(input [3:0] A, B, input cin, output [3:0] S, output cout);
    wire c1, c2, c3;
    full_adder_gate fa0(A[0], B[0], cin, S[0], c1);
    full_adder_gate fa1(A[1], B[1], c1,  S[1], c2);
    full_adder_gate fa2(A[2], B[2], c2,  S[2], c3);
    full_adder_gate fa3(A[3], B[3], c3,  S[3], cout);
endmodule
